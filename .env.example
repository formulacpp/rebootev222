# SellAuth Configuration
# Get your shop ID from the SellAuth dashboard
NEXT_PUBLIC_SELLAUTH_SHOP_ID=

# KeyAuth Configuration (Server-side only)
# These are used for both seller API and reseller login
KEYAUTH_NAME=your_app_name
KEYAUTH_OWNER_ID=your_owner_id
KEYAUTH_SECRET=your_app_secret
KEYAUTH_SELLER_KEY=your_seller_key

# External Services
NEXT_PUBLIC_DISCORD_INVITE=https://discord.gg/REBOOTED
NEXT_PUBLIC_STATUS_API=https://raw.githubusercontent.com/SunnyMonday45/Status/refs/heads/main/status.json
